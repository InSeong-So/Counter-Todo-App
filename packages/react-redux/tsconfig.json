{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "noEmit": false,
    "outDir": "dist",
    "target": "es2017",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "plugins": [
      {
        "name": "typescript-styled-plugin",
        "lint": {
          "validProperties": ["dominant-baseline"]
        }
      }
    ],
    "typeRoots": ["@types"],
  },
  "exclude": ["node_modules"]
}